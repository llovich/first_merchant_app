<!DOCTYPE html>
<html>
<head>
  <title>Merchant</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
  <div>
    <br>
    <%= link_to 'Products', admin_products_path, class:"btn btn-danger" %>
    <%= link_to 'Brands', admin_brands_path, class:"btn btn-danger" %>
    <%= link_to 'Categories', admin_categories_path, class:"btn btn-danger" %>
  </div>
  <%= yield %>
</div>

</body>
</html>
